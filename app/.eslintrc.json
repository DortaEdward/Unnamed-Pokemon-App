{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended",
    "airbnb-typescript",
    "airbnb-base"
  ],
  "rules": {
    "@typescript-eslint/consistent-type-imports": "warn",
    "import/extensions":0
  }
}